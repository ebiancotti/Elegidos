import Cantantes.*
import Jurados.*


test "jurado baboso da voto positivo si la cantante es joven" {

	var lola = new Cantante(25, "rap")
	var jurado1 = new Jurado(baboso)

	assert.equals(10, jurado1.votar(lola))

}

test "un jurado tradicionalista de voto positivo si el genero es propio del pais" {
	
		var cantanteTango = new Cantante(25, "tango")
		var juradoTradicional = new Jurado(tradicional)
		
		assert.equals(10, juradoTradicional.votar(cantanteTango))
	
}